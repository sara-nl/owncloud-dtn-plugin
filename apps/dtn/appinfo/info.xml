<info>
    <id>dtn</id>
    <name>DTN file transfer</name>
    <description>Transfer files through DTN</description>
    <licence></licence>
    <author>Antoon Prins</author>
    <namespace>DTN</namespace>
    <default_enable/>
    <version>0.1.0</version>
    <types>
        <filesystem/>
    </types>
    <dependencies>
        <owncloud min-version="10.0" max-version="10.0" />
    </dependencies>
    <documentation>
        <user>https://github.com/sara-nl/owncloud-dtn-plugin</user>
    </documentation>
    <settings>
        <personal>OCA\DTN\Panels\Personal</personal>
        <admin>OCA\DTN\Panels\Admin</admin>
    </settings>
</info>
